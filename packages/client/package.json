{
    "name": "@ragemp-mango/client",
    "version": "3.0.0",
    "homepage": "https://github.com/kkwlkk/ragemp-mango",
    "repository": {
        "type": "git",
        "url": "https://github.com/kkwlkk/ragemp-mango",
        "directory": "packages/client"
    },
    "license": "MIT",
    "author": "5exyGuy (AurÄ—jus Remeika), wlk (https://github.com/kkwlkk)",
    "description": "Client-side package for the RageMP Mango Framework - a decorator-driven TypeScript framework for RageMP servers.",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "default": "./dist/utils/index.js"
        }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit",
        "types": "tsup --dts-only"
    },
    "dependencies": {},
    "devDependencies": {
        "@abraham/reflection": "^0.12.0",
        "@ragemp-mango/core": "workspace:^",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@types/node": "^20.12.11",
        "esbuild": "^0.21.1",
        "rollup": "^4.17.2",
        "rollup-plugin-esbuild": "^6.1.1",
        "tsup": "^8.0.2",
        "typescript": "^5.4.5"
    },
    "peerDependencies": {
        "@ragemp-mango/core": "workspace:^",
        "@ragempcommunity/types-client": "^2.1.8",
        "inversify": "^6.0.2"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
